      "version": "~0.0.191107"
    },
    "tool-pyocd": {
      "type": "uploader",
      "optional": true,
      "version": "~0.801.0"
    },
    "tool-jlink": {
      "type": "uploader",
      "optional": true,
      "version": "^1.63208.0"
    },
    "tool-cmake": {
      "optional": true,
      "version": "~3.15.0"
    },
    "tool-dtc": {
      "optional": true,
      "version": "~1.4.7"
    },
    "tool-ninja": {
      "optional": true,
      "version": "^1.7.0"
    },
    "tool-gperf": {
      "optional": true,
      "version": "^3.0.0"
    }
  }
